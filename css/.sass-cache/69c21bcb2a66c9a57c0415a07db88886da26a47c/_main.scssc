			.circle{
				display: inline-block;
				vertical-align: text-bottom;
				height: 12px;
				width: 15px;
				border-radius: 7px;
				background: #f581c5;
				color: white;
				font-size: 8px;
				line-height: 1em;
				text-align: center;
				padding-top: 3px;
				margin-left: 5px;
			}
		}
	}
}
#timer {

	#nipple {
		height: 207px;
		width: 207px;
		background: #f59fd2;
		border-radius: 103px;
		margin: 42px auto 0 auto;
		margin: 25px auto 0 auto;
		text-shadow: none;
		text-align: center;
		position:relative;
		.inner {
			background: #f581c5;
			height: 63px;
			width: 63px;
			border-radius: 31px;
			position: absolute;
			top: 50%;
			left: 50%;
			margin-left: -31px;
			margin-top: -28px;
		}
		#tap {
			font-size: 21px;
			line-height: 1em;
			color: white;
			font-weight: normal;
			padding-top: 43px;
			z-index: 99999;
		}
		#clock {
			font-size: 64px;
			line-height: 1em;
			color: white;
			font-family: 'HelveticaNeue-UltraLight', 'Helvetica Neue UltraLight', 'Helvetica Neue', Arial, Helvetica, sans-serif;
			font-weight: 100;
			letter-spacing: 1px;
			position: absolute;
			left: 50%;
			margin-left: -81px;
			top: 50%;
			margin-top: -32px;
		}
	}
	.ui-content {
		position: relative;
	}
	.reminder {
		height: 44px;
		width: 44px;
		background: #f9c5e4;
		border-radius: 22px;
		color: white;
		font-size: 8px;
		text-align: center;
		display: table;
		position: absolute;			
		top:25px;
		span {
			display: table-cell;
			vertical-align: middle;
		}
	}
	.left {
		left:25px;
	}
	.right{
		right:25px;
	}
	#timer-save, #timer-reset {
		width: 66px;
		font-size: 9px;
		text-transform: uppercase;
		line-height: 1.4em;
		opacity: 0.2;
		transition: all 0.5s ease;
		-webkit-transition: all 0.5s ease;
		-moz-transition: all 0.5s ease;
		margin-top: -30px;
		img{
			height:50px;
			width:50px;
		}
	}
	#timer-save {
		float: left;
		text-align: left;
		margin-left: 10px;
	}
	#timer-reset {
		float: right;
		text-align: right;
		margin-right: 10px;
	}

	.selected {
		background: #f581c5;
		border: 4px solid #f3bbdc;
		border-radius: 26px;
		top: 21px;
	}
	.selected.right{
		right:21px;
	}
	.selected.left{
		left:21px;
	}
}
.ui-content {
	background: #ffffff;
	padding: 16px;
	li {
		color: black;
		font-size: 18px;
		line-height: 1.2em;
		border-bottom: 1px solid #cccccc;
		background: url('../images/arrow-right.jpg');
		background-repeat: no-repeat;
		background-position: right 20px top 20px;
		a{
			color: black;
			font-weight: normal;
			padding: 15px 35px 17px 20px;
			display:block;
			&:hover {
				text-decoration: none;
			}
			&:visited {
				color: black;
			}
		}
	}

	p {
		margin-bottom: 10px;
	}
}
/************************************************
*				MORE                            *
*************************************************/

.moreList.ui-content {
	padding: 0 20px !important;

	ul{
		li{
			padding:0;
			margin:0;
			margin-bottom: 10px;
			border: none;
			background: none;
			width: 100%;
			height: 60px;
			a{
				padding:0;
				margin:0;
				display: block;
				height: 60px;
				img{
					display: inline-block;
					vertical-align:middle;
					width:60px;
					margin-right: 12px;
				}
				span {
					margin: 20px 0 0 13px;
					display: inline-block;
				}
			}
		}
	}
	.heading {
		height: 31px;
		color:#4f4f4f;
		background: #e5e5e5;
		font-size: em(14);
		line-height: 1.1em;
		margin-bottom: 10px;
		margin-left: -20px;
		margin-right: -20px;
		padding: 0 20px;
		padding-top: 13px
	}
}

#about {
	font-size: 16px;
	line-height: 26px;
}
.moreList {
	-webkit-overflow-scrolling: touch;
}

.aboutBabyAvatarWrapper {
	height: 60px;
	width: 60px;
	overflow: hidden;
	border-radius: 30px;
	float: left;
	display: block;
	img {
		width: 100%;
		display: block;
	}
}

/************************************************
*				FOOTER                          *
*************************************************/

footer {
	background-color: #f8f8f8;
	position: absolute;
	bottom:0;
	left:0;
	right:0;
	height:85px;
	border-top: 1px solid #c6c6c6;
	text-align: center;
	display: none;
	ul {
		height: 100px;
		display: inline-block;
		font-size: 10px;
		line-height: 1.2em;			
		@media only screen and (max-width: 400px) {
			height: auto;
		}	
		li{
			float: left;
			margin-top: 17px;
			margin-right: 10px;
			text-transform: uppercase;
			width: 50px;
			a {
				color: #333333;
				&:hover {
					text-decoration: none;
				}
				img{
					border-radius: 27px;
					-moz-box-sizing:    border-box;
				   -webkit-box-sizing: border-box;
				    box-sizing:        border-box;
				    max-width: 100%;
				}
				img.selected {
					border: 2px solid black;
					//margin-top: -4px;
				}
				img.selected.advice, img.selected.diary {
					border: 2px solid #4f9898;
				}
				img.selected.timer, img.selected.more {
					border: 2px solid #e6bd36;
				}
				img.selected.map {
					border: 2px solid #c25d50;
				}
			}
		}
		
		li:nth-child(3){
			margin-top: 7px;
			width: 60px;
			img {
				border-radius: 32px;
			}
		}
		li:last-child{
			margin-right:0px;
		}
	}
}

