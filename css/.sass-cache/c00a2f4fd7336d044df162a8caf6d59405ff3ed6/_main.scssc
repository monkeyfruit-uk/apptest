footer {
	background-color: #f8f8f8;
	position: absolute;
	bottom:0;
	left:0;
	right:0;
	height:85px;
	border-top: 1px solid #c6c6c6;
	text-align: center;
	ul {
		height: 100px;
		display: inline-block;
		font-size: 10px;
		line-height: 1.2em;			
		@media only screen and (max-width: 400px) {
			height: auto;
		}	
		li{
			float: left;
			margin-top: 17px;
			margin-right: 10px;
			text-transform: uppercase;
			width: 50px;
			a {
				color: #333333;
				&:hover {
					text-decoration: none;
				}
				img{
					border-radius: 27px;
					-moz-box-sizing:    border-box;
				   -webkit-box-sizing: border-box;
				    box-sizing:        border-box;
				}
				img.selected {
					border: 2px solid black;
					//margin-top: -4px;
				}
				img.selected.advice, img.selected.diary {
					border: 2px solid #4f9898;
				}
				img.selected.timer, img.selected.more {
					border: 2px solid #e6bd36;
				}
				img.selected.map {
					border: 2px solid #c25d50;
				}
			}
		}
		
		li:nth-child(3){
			margin-top: 7px;
			width: 60px;
			img {
				border-radius: 32px;
			}
		}
		li:last-child{
			margin-right:0px;
		}
	}
}

